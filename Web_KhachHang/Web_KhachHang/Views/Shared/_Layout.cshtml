				<li data-target="#carousel" data-slide-to="2"></li>
				<li data-target="#carousel" data-slide-to="3"></li>
			</ol>
			<div class="carousel-inner" role="listbox">
				<div class="carousel-item active">
					<img style="width:1740px; height:700px; object-fit:cover;"  src="@Url.Content("~/images/slide/slide1.jpg")" alt="Slide #1" class="d-block img-fluid">
					<div class="carousel-caption">
					</div>
				</div>
				<div class="carousel-item">
					<img style="width:1740px; height:700px; object-fit:cover;" src="@Url.Content("~/images/slide/slide2.jpg")" alt="Slide #2" class="d-block img-fluid">
